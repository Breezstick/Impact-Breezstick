x: 0 0 calc(100% / 3); /* 3 images visibles en grand √©cran */\n    max-width: 300px;\n    background-color: #fff;\n    border-radius: 8px;\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n    overflow: hidden;\n    text-align: center;\n    scroll-snap-align: start;\n  }\n\n  .review-photo {\n    width: 100%;\n    height: auto;\n    display: block;\n  }\n\n  /* BARRE DE PROGRESSION */\n  .carousel-progress-bar {\n    width: 80%;\n    margin: 20px auto;\n    background-color: #e0e0e0;\n    height: 4px;\n    border-radius: 5px;\n    position: relative;\n  }\n  .carousel-progress-bar .progress {\n    width: 0%;\n    height: 100%;\n    background-color: #555;\n    border-radius: 5px;\n    transition: width 0.5s ease-in-out;\n  }\n\n  /* RESPONSIVE DESIGN */\n  @media (min-width: 768px) {\n    .review-card {\n      flex: 0 0 calc(33.33% - 20px);\n    }\n  }\n</style>\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function () {\n    const carousel = document.querySelector(\".carousel\");\n    const progressBar = document.querySelector(\".carousel-progress-bar .progress\");\n    const leftArrow = document.querySelector(\".left-arrow\");\n    const rightArrow = document.querySelector(\".right-arrow\");\n\n    const totalSlides = 8;\n    const slidesVisible = 3; // Nombre de photos visibles sur √©cran large\n    let currentSlide = 0;\n\n    function updateProgress() {\n      const progress = ((currentSlide + 1) / totalSlides) * 100;\n      progressBar.style.width = `${progress}%`;\n    }\n\n    function updateCarousel() {\n      const slideWidth = carousel.offsetWidth / slidesVisible;\n      const scrollAmount = slideWidth * currentSlide;\n\n      carousel.scrollTo({ left: scrollAmount, behavior: \"smooth\" });\n      updateProgress();\n    }\n\n    leftArrow.addEventListener(\"click\", () => {\n      if (currentSlide > 0) {\n        currentSlide--;\n        updateCarousel();\n      }\n    });\n\n    rightArrow.addEventListener(\"click\", () => {\n      if (currentSlide < totalSlides - slidesVisible) {\n        currentSlide++;\n        updateCarousel();\n      }\n    });\n\n    // D√©tecte le d√©filement manuel\n    carousel.addEventListener(\"scroll\", () => {\n      const slideWidth = carousel.offsetWidth / slidesVisible;\n      currentSlide = Math.round(carousel.scrollLeft / slideWidth);\n      updateProgress();\n    });\n\n    // Initialisation de la progression\n    updateProgress();\n  });\n</script>\n</div>",
